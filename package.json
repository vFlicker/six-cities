{
  "name": "six-cities",
  "version": "1.0.0",
  "author": "Vladislav Slyusar",
  "private": true,
  "homepage": "https://github.com/vFlicker/six-cities",
  "scripts": {
    "start": "webpack-dev-server --env mode=development",
    "build": "webpack --env mode=production",
    "predeploy" : "npm run build",
    "deploy" : "gh-pages -d public",
    "lint": "eslint src --ext .ts,.tsx --fix",
    "test": "jest"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "jest": {
    "resetMocks": false,
    "setupFiles": [
      "jest-localstorage-mock"
    ],
    "testEnvironment": "jsdom",
    "moduleNameMapper": {
      "^~(.*)$": "<rootDir>/src$1",
      "\\.(jpg|ico|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/tests/file-mock.ts",
      "\\.(css|less)$": "<rootDir>/src/tests/file-mock.ts"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@emotion/react": "11.10.5",
    "@emotion/styled": "11.10.5",
    "@reduxjs/toolkit": "1.9.0",
    "axios": "0.24.0",
    "history": "5.3.0",
    "http-status-codes": "2.2.0",
    "humps": "2.0.1",
    "leaflet": "1.7.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-redux": "8.0.5",
    "react-router-dom": "6.4.3",
    "react-toastify": "9.0.1"
  },
  "devDependencies": {
    "@babel/core": "7.15.5",
    "@babel/plugin-transform-runtime": "7.15.0",
    "@babel/preset-env": "7.20.2",
    "@babel/preset-react": "7.18.6",
    "@babel/preset-typescript": "7.15.0",
    "@babel/runtime": "7.15.4",
    "@faker-js/faker": "7.6.0",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@testing-library/user-event": "14.4.3",
    "@types/humps": "2.0.1",
    "@types/leaflet": "1.7.5",
    "@types/node": "18.11.9",
    "@types/react-toastify": "4.1.0",
    "@types/redux-mock-store": "1.0.3",
    "@types/webpack": "5.28.0",
    "@types/webpack-dev-server": "4.1.0",
    "@typescript-eslint/eslint-plugin": "4.31.0",
    "@typescript-eslint/parser": "4.31.0",
    "axios-mock-adapter": "1.21.2",
    "babel-jest": "29.3.1",
    "babel-loader": "8.2.2",
    "babel-plugin-styled-components": "2.0.7",
    "cross-env": "7.0.3",
    "css-loader": "6.4.0",
    "css-minimizer-webpack-plugin": "4.0.0",
    "eslint": "7.32.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-htmlacademy": "4.0.2",
    "eslint-config-prettier": "8.5.0",
    "eslint-import-resolver-webpack": "0.13.1",
    "eslint-plugin-import": "2.24.2",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.25.1",
    "eslint-plugin-react-hooks": "4.3.0",
    "file-loader": "6.2.0",
    "fork-ts-checker-webpack-plugin": "6.3.3",
    "gh-pages": "4.0.0",
    "html-webpack-plugin": "5.5.0",
    "image-minimizer-webpack-plugin": "3.2.3",
    "imagemin-pngquant": "9.0.2",
    "jest": "29.3.1",
    "jest-environment-jsdom": "29.3.1",
    "jest-localstorage-mock": "2.4.22",
    "mini-css-extract-plugin": "2.6.0",
    "prettier": "2.7.1",
    "react-test-renderer": "18.2.0",
    "redux-mock-store": "1.5.4",
    "redux-thunk": "^2.4.2",
    "style-loader": "3.3.0",
    "terser-webpack-plugin": "5.3.1",
    "ts-node": "10.2.1",
    "typescript": "4.4.2",
    "webpack": "5.52.0",
    "webpack-cli": "4.8.0",
    "webpack-dev-server": "4.1.1"
  }
}
